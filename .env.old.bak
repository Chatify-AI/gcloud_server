# Server Configuration for Remote Access
PORT=3000
NODE_ENV=development

# Database Configuration
# SQLite database will be created automatically in database/gcloud_manager.sqlite

# Google OAuth Configuration
# You need to create OAuth 2.0 credentials in Google Cloud Console
# Go to: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
# Update this with your server's public IP or domain
GOOGLE_REDIRECT_URI=http://2a02:4780:10:2410::1:3000/api/auth/google/callback

# JWT Configuration
JWT_SECRET=your_very_secure_jwt_secret_key_here_change_this_in_production
