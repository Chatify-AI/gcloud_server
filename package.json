{
  "name": "gcloud-server",
  "version": "1.0.0",
  "description": "Google Cloud Management Server with OAuth and Command Execution",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "nodemon backend/src/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "gcloud",
    "oauth",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "connect-sqlite3": "^0.9.16",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.18.2",
    "google-auth-library": "^9.2.0",
    "googleapis": "^128.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.0",
    "node-pty": "^1.0.0",
    "sequelize": "^6.33.0",
    "socket.io": "^4.5.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
